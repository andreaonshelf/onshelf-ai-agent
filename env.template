# Agent Configuration (optional - defaults shown)
# TARGET_ACCURACY=0.95
# MAX_ITERATIONS=5
# MAX_PROCESSING_TIME_SECONDS=300
# MAX_API_COST_PER_EXTRACTION=1.00

# WebSocket Configuration (optional)
